<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="staclogintest.kr.hs.mirim.alone_test">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".PlaylistActivity" />
        <activity android:name=".LibraryActivity" />
        <activity android:name=".CommunityActivity"/>
        <activity android:name=".SignInActivity"/>
        <activity android:name=".PlaylistContentActivity"/>
        <activity android:name=".SongListActivity"/>
        <activity android:name=".StopActivity"/>
        <activity android:name=".CreateMemoActivity"/>

        <service
            android:name=".service.AudioService"
            android:enabled="true"
            android:exported="true" />

    </application>

</manifest>